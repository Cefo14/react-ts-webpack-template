{
  "name": "react-ts-webpack-template",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "webpack serve --mode development --define-process-env-node-env development",
    "build": "webpack --mode production --define-process-env-node-env production --progress",
    "build:analyze": "npm run build -- --env ANALYZE_BUNDLE",
    "lint": "eslint src --ext .ts --ext .tsx",
    "lint:fix": "npm run lint -- --fix",
    "lint:styles": "stylelint src/**/*.{css,scss}",
    "lint:styles:fix": "npm run lint:styles -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:staged": "jest --bail --findRelatedTests --passWithNoTests",
    "git:husky": "husky install",
    "git:commit": "cz",
    "check:updates": "npx npm-check-updates",
    "check:unused": "npx depcheck",
    "check:unimported": "npx unimported",
    "lighthouseci": "npx @lhci/cli autorun",
    "postinstall": "npm run git:husky"
  },
  "devDependencies": {
    "@babel/core": "7.21.0",
