    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "babel-loader": "9.1.2",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-transform-imports": "2.0.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
